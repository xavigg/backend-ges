    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/platform-fastify": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
